{
  "name": "test-tag-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "install:all": "npm install && npm i react-konva konva zustand recharts jspdf html2canvas react-router-dom && npm i -D @types/konva @types/jest vitest @testing-library/react @testing-library/user-event @testing-library/jest-dom eslint prettier",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0 || true",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "start:run": "zsh ./run.sh"
  },
  "dependencies": {
    "html-to-image": "^1.10.7",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "konva": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-konva": "^18.0.7",
    "react-router-dom": "^6.14.1",
    "recharts": "^3.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^1.0.0"
  }
}
